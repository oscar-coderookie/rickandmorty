{
  "expo": {
    "name": "Rick & Morty App",
    "slug": "rickandmortyapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#00B0C8"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "First app developed in react native, using props and fetch api.",
    "githubUrl": "https://github.com/oscar-coderookie/rickandmorty.git"
  }
}
